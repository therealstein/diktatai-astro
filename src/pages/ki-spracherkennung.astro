---
import Layout from '../layouts/Layout.astro';
import AiSpeechRecognitionPage from '../components/AiSpeechRecognitionPage.astro';
import { aiSpeechRecognitionTranslations } from '../i18n/aiSpeechRecognition';

const locale = 'de';
const t = aiSpeechRecognitionTranslations[locale];
---

<Layout
  title={t.seo.title}
  description={t.seo.description}
  locale={locale}
  routeKey="aiSpeechRecognition"
>
  <AiSpeechRecognitionPage locale={locale} t={t} />
</Layout>
